  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**'",
    "preview": "vite preview",
    "codegen": "yarn codegen:download && graphql-codegen",
    "codegen:download": "node scripts/codegen/download.mjs",
    "lint:fix": "eslint --ext .js,.ts,.tsx 'src/**' --fix",
    "test": "jest src/",
    "test:ci": "jest src/ --coverage",
    "test:watch": "jest --watch src/",
    "test:components": "jest --testPathPattern='\\.test\\.tsx$'",
    "tsc": "tsc",
    "ts-node": "ts-node",
    "tsc:scripts": "tsc --p ./scripts/",
    "format:code": "npx prettier --ignore-unknown --write src/*",
    "gen:atom": "ts-node ./scripts/newChakraAtomComponent.ts",
    "new:mutationMock": "ts-node ./scripts/newMutationMock.ts",
    "new:queryMock": "ts-node ./scripts/newQueryMock.ts",
    "generateGraphQLFiles": "ts-node ./scripts/newGraphQLFiles.ts",
    "preinstall": "node -e \"if (!/yarn/.test(process.env.npm_execpath || '')) process.exit(1)\"",
    "prepare": "husky",
    "check-types:src": "tsc --noEmit && tsc-strict",
    "gen:theme-typings": "chakra typegen ./src/shared/chakra/themeSystem.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "check-storybook-health": "storybook doctor",
    "chromatic": "npx chromatic --project-token=chpt_0373c8385e43981",
    "check-types:playwright": "tsc --noEmit --project playwright/tsconfig.json",
    "check-types": "npm run check-types:src && npm run check-types:playwright",
    "qa:pw-e2e": "npx playwright test --grep \"#e2e\"",
    "qa:pw-ui": "npx playwright test --ui"
  },
